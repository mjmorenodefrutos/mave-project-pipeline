pipeline {
    agent any

    stages {
        stage('Inicial') {
            steps {
                echo 'Estoy en la fase Inicial'
            }
        }
        stage('Etapa 2') {
            steps {
                echo 'Estoy en etapa 2'
            }
        }
        stage('Etapa 3') {
            steps {
                echo 'Hola Mundo'
            }
        }
    }
}